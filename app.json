      "package": "com.manojtsx.chatbot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7eb0887d-5af4-4eb6-beca-8a4abb45d3f9"
